#!/usr/bin/env python
# -*- coding: utf-8 -*-

from math import *


class vector:

    def __init__(self, x, y):
        self.x, self.y = x, y


    def intone(self):
        return vector(self.x / self.abs(), self.y / self.abs())

    def dot_product(self, v):
        v1 = self
        v2 = v

        r1 = v1.abs()
        r2 = v2.abs()

        r1 * r2
